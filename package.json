{
  "name": "gitbook-plugin-tabs",
  "description": "Clickable tabs for Gitbook pages",
  "version": "1.0.13",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "gitbook": ">=2.4.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "gitbook": {
        "properties": {
            "myConfigKey": {
                "type": "string",
                "default": "it's the default value",
                "description": "It defines my awesome config!"
            }
        },
        "blocks": {
            "muti-column": {
                "title": "Muti Column Layout",
                "description": "Display sub-blocks in muti-column. 用分栏的方式展示内容",
                "properties": {
                    "alignment": {
                        "type": "string"
                    }
                }
            }
        }
    },
  "dependencies": {
    "gitbook-markdown": "^1.1.1"
  }
}
